{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "printWidth": 80,
  "tabWidth": 2,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "objectPropertyNewline": "allowAllPropertiesOnSameLine",
  "endOfLine": "auto",
  "bracketSameLine": false,
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" }
}
